# Focus windows
cmd + alt + ctrl - left  : yabai -m window --focus west
cmd + alt + ctrl - right : yabai -m window --focus east
cmd + alt + ctrl - up    : yabai -m window --focus north
cmd + alt + ctrl - down  : yabai -m window --focus south

# Swap windows
cmd + alt + ctrl + shift - left  : yabai -m window --swap west
cmd + alt + ctrl + shift - right : yabai -m window --swap east
cmd + alt + ctrl + shift - up    : yabai -m window --swap north
cmd + alt + ctrl + shift - down  : yabai -m window --swap south

# Resize windows
cmd + alt + ctrl + shift - left  : yabai -m window --resize left:-20:0
cmd + alt + ctrl + shift - right : yabai -m window --resize right:20:0
cmd + alt + ctrl + shift - up    : yabai -m window --resize top:0:-20
cmd + alt + ctrl + shift - down  : yabai -m window --resize bottom:0:20
